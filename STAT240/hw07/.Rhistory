p1 = tibble(x = x, p = c(p, rep(NA,2)))
p1_tab = p1 %>%
mutate(x = str_c(x)) %>%
pivot_wider(names_from = x, values_from = p) %>%
mutate(x = "P(X=x)", .before = `0`)
p1_tab %>%
kable() %>%
kable_styling(position = "center", full_width = FALSE)
