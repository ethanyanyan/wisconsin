ord = map_dfr(2017:2021, ~{
return ( read_csv(str_c("../../data/ORD-", .x, ".csv")) )
})
ord = ord %>%
# Add new date-related columns
mutate(
year  = year(date),
month = month(date, label = TRUE, abbr = TRUE),
day   = day(date),
wday  = wday(date, label = TRUE)
) %>%
# Reorder variables so new columns appear directly after the date column
select(airport:date, year:wday, everything()) %>%
# Remove 'terminal' column
select(-terminal) %>%
# Rename specified columns
rename(
passengers = all_total,
flights    = all_flights,
booths     = all_booths
) %>%
# Arrange rows by date and hour
arrange(date, hour)
# Print the first 5 rows
head(ord, 5)
# Print the dimensions of the modified dataframe
dim(ord)
date_range = seq(from = ymd("2017-01-01"), to = ymd("2021-12-31"), by = "day")
distinct_dates = ord %>% distinct(date) %>% pull(date)
missing_dates = setdiff(date_range, distinct_dates)
formatted_missing_dates = as.Date(missing_dates, origin="1970-01-01")
cat("How many missing dates?", length(missing_dates), "\n")
cat("The missing dates are: ", formatted_missing_dates)
